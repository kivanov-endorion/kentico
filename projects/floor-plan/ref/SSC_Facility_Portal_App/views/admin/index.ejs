                <table class="table table-sm" id="teams">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Team</th>
                            <th>Team Color</th>
                            <th>Team Text Color</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% teams.forEach(function (team) { %>
                        <tr>
                            <td>
                                <%= team.Id %>
                            </td>
                            <td>
                                <%= team.Team %>
                            </td>
                            <form action="/users/teamColorUpdate" method="post">
                                <td>
                                    <% if (team.TeamColor) { %>
                                    <input class="form-control form-control-sm" style="height: 30px;" type="color"
                                        name="TeamColor" value="<%= team.TeamColor %>" />
                                    <% } else { %>
                                    <input class="form-control form-control-sm" style="height: 30px;" type="color"
                                        name="TeamColor" value="#FFFFFF" />
                                    <% } %>
                                    <input type="hidden" readonly name="Team" value="<%= team.Team %>" />
                                </td>
                                <td>
                                    <% if (team.TeamTextColor) { %>
                                    <input class="form-control form-control-sm" style="height: 30px;" type="color"
                                        name="TeamTextColor" value="<%= team.TeamTextColor %>" />
                                    <% } else { %>
                                    <input class="form-control form-control-sm" style="height: 30px;" type="color"
                                        name="TeamTextColor" value="#000000" />
                                    <% } %>
                                </td>
