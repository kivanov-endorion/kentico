        <script src="https://d3js.org/d3.v5.js"></script> -->
        <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.3/dist/Chart.min.js"></script>
        <div id="collapseFive" class="collapse" aria-labelledby="shifts" data-parent="#accordion">
            <div class="card-body">
                <div class="row">

                    <!-- <div id="chartShifts" class='with-transitions'>
