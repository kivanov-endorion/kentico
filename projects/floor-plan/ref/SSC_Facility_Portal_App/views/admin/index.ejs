<% if (userType == "superadmin") { %>
<div id="accordion">
    <div class="card">
        <div class="card-header" id="headingOne">
            <h5 class="mb-0">
                <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true"
                    aria-controls="collapseOne">
                    Location Places Number
                </button>
            </h5>
        </div>

        <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                <div class="row-fluid">
                    <div class="col-md-12 order-md-1">
                        <table class="table table-sm">
                            <thead>
                                <th>Id</th>
                                <th>BuildingId</th>
                                <th>Open Area Places</th>
                                <th>Offices Places</th>
                                <th>Total</th>
                                <th></th>
                            </thead>
                            <tbody>
                                <% places.forEach(function (place) { %>
                                <tr>
                                    <form action="/users/updateLocation" method="post">
                                        <td><input class="form-control form-control-sm" type="text" name="placeId"
                                                value="<%= place.ID %>" readonly="true" /></td>
                                        <td><input class="form-control form-control-sm" type="text" name="buildingName"
                                                value="<%= place.BuildingName %>" readonly="true" /></td>
                                        <td><input class="form-control form-control-sm" type="text"
                                                name="openAreaPlaces" value="<%= place.OpenAreaPlaces %>" /></td>
                                        <td><input class="form-control form-control-sm" type="text" name="officesPlaces"
                                                value="<%= place.OfficesPlaces %>" /></td>
                                        <td><input class="form-control form-control-sm" type="text" name="total"
                                                value="<%= place.Total %>" /></td>
                                        <td><input class="btn btn-sm btn-success" type="submit" value="Update" /></td>
                                    </form>
                                </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="card">
        <div class="card-header" id="headingTwo">
            <h5 class="mb-0">
                <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseTwo"
                    aria-expanded="false" aria-controls="collapseTwo">
                    Admins/Super Admins List
                </button>
            </h5>
        </div>
        <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="box box-bordered box-condensed">
                            <div class="">
                                <h3>Super Admins List</h3>
                            </div>

                            <div class="">
                                <form action="/users/createSuperAdmin" method="POST"
                                    class="form-horizontal form-column form-bordered form-condensed">
                                    <div class="row-fluid">
                                        <div class="span12">
                                            <div class="control-group">
                                                <div class="controls">
                                                    <input class="form-control form-control-sm" type="text"
                                                        name="username" placeholder="Add Username..." />
                                                </div>
                                            </div>
                                            <button class="btn-block btn btn-sm btn-success" type="submit">Create Super
                                                Admin</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="box-content nopadding">
                                <input class="form-control form-control-sm" style="margin-top: 20px;" type="text"
                                    id="superAdminSearch" onkeyup="myFunction('superAdminSearch', 'superAdminsList')"
                                    placeholder="Search for super admin..">
                                <table id="superAdminsList" class="table table-sm">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>Username</th>
                                            <th>Remove Admin</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% superAdminsList.forEach(function (admin) { %>
                                        <tr>
                                            <td>
                                                <%= admin.Id %>
                                            </td>
                                            <td>
                                                <%= admin.Username %>
                                            </td>
                                            <td>
                                                <form action="/users/removeSuperAdmin" method="post">
                                                    <input type="hidden" readonly name="userId"
                                                        value="<%= admin.Id %>" />
                                                    <input class="btn btn-sm btn-danger" type="submit" value="Remove" />
                                                </form>
                                            </td>
                                        </tr>
                                        <% }); %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="box box-bordered box-condensed">
