                    </div> -->
                    <canvas id="totalChart" width="400" height="100"></canvas>
                    <button class="btn btn-success rounded-0 mx-2" onclick="exportFunc('green');">Export Green Shifts</button>
                    <button class="btn btn-danger rounded-0 mx-2" onclick="exportFunc('red');">Export Red Shifts</button>
                    <button class="btn btn-warning rounded-0 mx-2" onclick="exportFunc('yellow');">Export Yellow Shifts</button>
                    <script>
                        $.ajax({
                            cache: false,
                            type: "GET",
                            url: "/location/getShiftsChartInformation",
                            dataType: "json"
                        }).done(function (res) {
                            var ctx = document.getElementById('totalChart').getContext('2d');
                            var inLabels = res.total.map(function (obj) { return obj.ShiftColor; });
                            var colorsArr = [
                                'rgba(255, 99, 132, 0.2)', // red
                                'rgba(255, 206, 86, 0.2)', // yellow
                                'rgba(75, 192, 192, 0.2)', // green
                            ];
                            var inColors = [];
                            var finalObjects = [];
                            for (var i = 0; i < inLabels.length; i++) {
                                if (inLabels[i] == 'green') {
                                    inColors.push(colorsArr[2]);
                                } else if (inLabels[i] == 'red') {
                                    inColors.push(colorsArr[0]);
                                } else if (inLabels[i] == 'yellow') {
                                    inColors.push(colorsArr[1]);
                                }
                            }
                            finalObjects.push({
                                label: 'Total # of Shifts',
                                data: res.total.map(function (obj) { return obj.TotalCnt; }),
                                backgroundColor: inColors,
                                borderColor: [],
                                borderWidth: 1
                            });
                            var additionalValues = res.separate.map(function (obj) {
                                return obj.BuildingName;
                            }).filter(function (item, i, ar) { return ar.indexOf(item) == i; });
                            for (var i = 0; i < additionalValues.length; i++) {
                                if (inLabels[i] == 'green') {
                                    // inColors.push(colorsArr[2]);
                                } else if (inLabels[i] == 'red') {
                                    // inColors.push(colorsArr[0]);
                                } else if (inLabels[i] == 'yellow') {
                                    // inColors.push(colorsArr[1]);
                                }
                            }

                            var totalChart = new Chart(ctx, {
                                type: 'bar',
                                data: {
                                    labels: inLabels,
                                    datasets: finalObjects
                                },
                                options: {
                                    scales: {
                                        yAxes: [{
                                            ticks: {
                                                beginAtZero: true
                                            }
                                        }]
                                    }
                                }
                            });
                        });
                        function exportFunc(inParam) {
                            window.open(window.location.origin + "/location/extractShiftsInformation?shift=" + inParam, "_blank");
                        }
                    </script>
                </div>
            </div>
        </div>
    </div>
</div>

</div>
